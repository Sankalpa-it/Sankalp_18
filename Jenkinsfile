pipeline {
    agent any

    stages {
        stage('Clone code from Git') {
            steps {
                echo 'Code Clone Succufully'
            }
            stage('Buid Code') {
            steps {
                echo 'Code built Succufully '
            }
            stage('Code  Package') {
            steps {
                echo 'Code Packaged'
            }
            stage('Test Code') {
            steps {
                echo 'Testing Success'
            }
            stage('Deploy Code') {
            steps {
                echo 'Code deploy success'
        }
    }
}
